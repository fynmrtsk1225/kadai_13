<h1>検索結果</h1>

<table>
  <section class="all_pictures_main">
    <tbody class="picture search_picture">
      <% @results.each do |user| %>
        <tr id="user_<%= user.id %>">
          <%= render 'user', user: user %>
        </tr>
      <% end %>
    </tbody>
  </section>
</table>

<%= link_to '全ユーザ一覧', users_path %>