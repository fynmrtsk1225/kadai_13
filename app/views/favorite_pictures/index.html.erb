<div class="all_picture">
  <h1>Favorites</h1>

  <section class="all_pictures_main">
    <table>
      <%# <thead>
        <tr>
          <th>Image</th>
          <th>Content</th>
          <th>User</th>
          <th colspan="3"></th>
        </tr>
      </thead> %>

      <tbody class="picture">
        <% @favorite_pictures.each do |picture| %>
          <tr id="picture_<%= picture.id %>">
            <%= render '/pictures/picture', picture: picture %>
          </tr>
        <% end %>
      </tbody>
    </table>
  </section>
</div>